#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/redis-memory-server@0.14.0/node_modules/redis-memory-server/bin/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/redis-memory-server@0.14.0/node_modules/redis-memory-server/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/redis-memory-server@0.14.0/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/redis-memory-server@0.14.0/node_modules/redis-memory-server/bin/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/redis-memory-server@0.14.0/node_modules/redis-memory-server/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/redis-memory-server@0.14.0/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../redis-memory-server@0.14.0/node_modules/redis-memory-server/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../redis-memory-server@0.14.0/node_modules/redis-memory-server/bin/index.js" "$@"
fi
