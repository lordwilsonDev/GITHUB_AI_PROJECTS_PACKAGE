#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules/motia/dist/cjs/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules/motia/dist/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules/motia/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules/motia/dist/cjs/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules/motia/dist/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules/motia/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/motia@0.13.1-beta.162_@types+node@24.10.1_eslint@9.39.1_jiti@2.6.1__jiti@2.6.1_lightnin_15ee76ab0e1bfeec3a03db15f9bb1f09/node_modules:/Users/lordwilson/motia-recursive-agent/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cjs/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cjs/cli.js" "$@"
fi
